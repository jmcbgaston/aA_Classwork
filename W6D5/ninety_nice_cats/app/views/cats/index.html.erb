
<h1>Ninety Nice Cats</h1>

<table>
  <colgroup span="4"></colgroup>
  <tr>
    <th>Name</th>
    <th>Birthdate</th>
    <th>Color</th>
    <th>Sex</th>
    <th>Description</th>
  </tr>
    <% @cats.each do |cat| %>
  <tr>
        <td><%= cat.name %></td>
        <td><%= cat.birth_date %></td>
        <td><%= cat.color %></td>
        <td><%= cat.sex %></td>
        <td><%= cat.description %></td>
  </tr>
    <% end %>
</table>
